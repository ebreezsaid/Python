   "source": [
    "def find(username):\n",
    "    if len(username) % 2 == 1:\n",
    "        return \"boy\"\n",
    "    else:\n",
    "        return \"girl\"\n",
    "\n",
    "\n",
    "username = input(\"Enter the username: \")\n",
    "\n",
    "\n",
    "gender = find(username)\n",
    "\n",
    "print(f\"The gender of the user with username '{username}' is: {gender}\")"
   ]
